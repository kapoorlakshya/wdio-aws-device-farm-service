  },
  "dependencies": {
    "@wdio/logger": "^5.0.0",
    "@wdio/cli": "^5.0.0",
    "aws-sdk": "^2.646.0",
    "webdriverio": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
